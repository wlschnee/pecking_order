
The search results!
  <div class="result-wrapper">
    <table class="table table-hover">
      <tr>
        <th>Name</th>
        <th>Address</th>
      </tr>
      <% @results.each_with_index do |result, index| %>
        <tr class="result", id="<%= index %>">
          <td id="name_<%= index %>"><%= result.name %></td>
          <td id="address_<%= index %>"><%= result.location.display_address.join(', ') %></td>
        </tr>
      <% end %>
    </table>
  </div>
