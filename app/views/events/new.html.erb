<h1>Create a new event</h1>

<div class="row">
  <div class="col-md-6">
    <%= render 'new_form' %>
  </div><!-- left-col -->
  <div class="col-md-6" id="right-col">
    <%= form_tag '/search', remote: true do %>
      <%= label_tag 'search', 'Search' %>
      <%= text_field_tag 'search', nil, placeholder: 'powered by Yelp', width: "100%" %>
      <%= submit_tag 'Search', class: "btn btn-default" %>
    <% end %>
    <div class="search_results">
    </div>
  </div><!-- right-col -->
</div><!-- row -->
