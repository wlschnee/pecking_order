<h2>Create a new user!</h2>

<%= form_for @user do |f| %>
<div>
  <%= f.label :email %>
  <%= f.text_field :email %>
</div>
<div>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
</div>
<div>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
</div>
  <%= f.label :password_digest %>
  <%= f.text_field :password_digest %>
<div>
  <%= f.submit "Create Account" %>
</div>
  <% end %>